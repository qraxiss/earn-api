    "tasks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::task.claimed-task",
      "mappedBy": "user"
    },
    "dailies": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::daily.daily",
      "mappedBy": "user"
    },
    "stack": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::card.stack",
      "mappedBy": "user"
    },
    "xp": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::xp.xp",
      "mappedBy": "user"
    },
    "referrer": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::referrer.referrer",
      "mappedBy": "user"
